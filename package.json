{
  "name": "faishon-desaign-client",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --port 4200 --host 0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node index.js",
    "postinstall": "node build/pre-build.js && rm -rf dist && npm run build",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build-prod-ngsw": "ng build --prod && npm run ngsw-config && npm run ngsw-copy",
    "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 8080"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.1",
    "@angular/cdk": "^5.2.5",
    "@angular/cli": "^6.0.1",
    "@angular/common": "^6.0.1",
    "@angular/compiler": "^6.0.1",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/core": "^6.0.1",
    "@angular/forms": "^6.0.1",
    "@angular/http": "^6.0.1",
    "@angular/language-service": "^6.0.1",
    "@angular/material": "^5.2.5",
    "@angular/material-moment-adapter": "^6.0.1",
    "@angular/platform-browser": "^6.0.1",
    "@angular/platform-browser-dynamic": "^6.0.1",
    "@angular/pwa": "^0.7.0-beta.1",
    "@angular/router": "^6.0.1",
    "@angular/service-worker": "^6.0.1",
    "@types/jquery": "^3.3.0",
    "bootstrap": "^4.0.0-beta.3",
    "colors": "^1.3.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.20.1",
    "path": "^0.12.7",
    "popper.js": "^1.12.9",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "tslib": "1.9.0",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1"
  }
}
